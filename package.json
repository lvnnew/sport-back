  "scripts": {
    "serve": "serve public",
    "build": "(rm -rf dist || true) && tsc",
    "start": "runlify start env=prod node --unhandled-rejections=strict dist/index.js",
    "dev": "ts-node-dev --files src/index.ts",
    "dev:stage": "runlify start env=stage yarn dev",
    "lint": "eslint ./src/**/*.ts",
    "gen": "yarn ts-node src/gen/genGQSchemes.ts",
    "test": "jest --maxWorkers 8",
    "prisma:gen": "prisma generate",
    "prisma:newMigration": "runlify start env=migration prisma migrate dev --preview-feature",
    "prisma:deploy": "runlify start prisma migrate deploy --preview-feature",
    "prisma": "prisma",
    "ts-node:withContext": "ts-node ./src/init/wrap.ts",
    "init:base": "ts-node src/init/baseInit.ts",
    "init:permissions": "yarn ts-node:withContext src/init/permissions/initPermissions.ts",
    "init:dev": "ts-node src/init/initDev.ts",
    "regen": "yarn ts-node src/meta/regenBasedOnMeta.ts && runlify regen",
    "preinstall": "npx only-allow yarn"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/"
    ],
    "setupFilesAfterEnv": [
      "jest-extended/all"
    ],
    "testTimeout": 120000
  },
