  "scripts": {
    "serve": "serve public",
    "build_": "tsc",
    "build": "(rm -rf dist || true) && tsc",
    "start": "NODE_ENV=production node --unhandled-rejections=strict dist/index.js",
    "dev": "ts-node-dev src/index.ts",
    "dev:stage": "AGR_CONFIG_PATH='' yarn dev",
    "dev:nt": "AGR_TOKEN='' ts-node-dev src/index.ts",
    "dev:nw": "ts-node src/index.ts",
    "ts": "node --loader ts-node/esm.mjs --experimental-specifier-resolution=node",
    "test:lint": "eslint ./src/**.ts",
    "g:dev": "ts-node-dev src/index.ts",
    "g:start:cli": "NODE_ENV=production pm2-runtime start dist/index.js",
    "g:stop": "NODE_ENV=production pm2 stop dist/index.js",
    "gen": "graphql-codegen --config codegen.yml",
    "studio": "prisma studio --experimental",
    "migrate:save": "prisma migrate save --experimental",
    "migrate:up": "prisma migrate up --experimental",
    "test": "jest --maxWorkers 8",
    "prisma:gen": "prisma generate"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/"
    ]
